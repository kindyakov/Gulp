{
  "trailingComma": "none",
  "tabWidth": 2,
  "useTabs": true,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "avoid",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "/modules/",
    "^../(.*)",
    "^./(.*)"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true
}